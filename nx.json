{
  "npmScope": "fedex",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "wcc": {
      "tags": ["type:app", "app:wcc"]
    },
    "wcc-e2e": {
      "tags": ["type:e2e"],
      "implicitDependencies": ["wcc"]
    },
    "shared-design-system-ui": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-dashboard-feature-dashboard": {
      "tags": ["scope:shared", "type:feature"]
    },
    "shared-dashboard-data-access": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "wcc-feature-shell": {
      "tags": ["scope:wcc", "type:feature"]
    },
    "shared-design-system-ui-e2e": {
      "tags": [],
      "implicitDependencies": ["shared-design-system-ui"]
    },
    "shared-iam-data-access": {
      "tags": ["scope:shared", "type:data-access"]
    },
    "wcc-feature-iam": {
      "tags": ["scope:wcc", "type:feature"]
    },
    "shared-iam-util": {
      "tags": ["scope:shared", "type:util"]
    },
    "shared-design-system-icons": {
      "tags": ["scope:shared", "type:ui"]
    },
    "shared-utils": {
      "tags": ["scope:shared", "type:util"]
    },
    "shared-mock-server": {
      "tags": ["scope:shared", "type:util"]
    }
  }
}
