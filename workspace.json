  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@ngneat/spectator:spectator-component": {
      "changeDetection": "OnPush",
      "displayBlock": true,
      "jest": true,
      "style": "scss"
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
